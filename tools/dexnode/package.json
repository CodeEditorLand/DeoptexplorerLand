{
  "name": "dexnode",
  "version": "1.2.2",
  "description": "Run NodeJS with logging options needed by Deopt Explorer",
  "type": "module",
  "bin": {
    "dexnode": "./bin/dexnode.js"
  },
  "keywords": [
    "deopt",
    "explorer",
    "nodejs",
    "v8"
  ],
  "author": "Ron Buckton <ron.buckton@microsoft.com>",
  "license": "MIT",
  "dependencies": {
    "semver": "7.5.4",
    "winreg": "1.2.5"
  },
  "devDependencies": {
    "@types/semver": "7.5.5",
    "@types/winreg": "1.2.35",
    "typescript": "5.2.2"
  },
  "scripts": {
    "compile": "tsc -b",
    "clean": "tsc -b --clean",
    "prestart": "npm run compile",
    "start": "node bin/dexnode"
  }
}
