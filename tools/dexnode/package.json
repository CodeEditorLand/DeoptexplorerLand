{
	"name": "dexnode",
	"description": "Run NodeJS with logging options needed by Deopt Explorer",
	"bin": {
		"dexnode": "./bin/dexnode.js"
	},
	"scripts": {
		"clean": "tsc -b --clean",
		"compile": "tsc -b",
		"prestart": "npm run compile",
		"start": "node bin/dexnode"
	},
	"dependencies": {
		"semver": "7.5.2",
		"winreg": "1.2.4"
	},
	"devDependencies": {
		"@types/semver": "7.3.13",
		"@types/winreg": "1.2.32"
	}
}
