{
	"name": "dexnode",
	"version": "1.2.2",
	"description": "Run NodeJS with logging options needed by Deopt Explorer",
	"keywords": [
		"deopt",
		"explorer",
		"nodejs",
		"v8"
	],
	"license": "MIT",
	"author": "Ron Buckton <ron.buckton@microsoft.com>",
	"type": "module",
	"bin": {
		"dexnode": "./bin/dexnode.js"
	},
	"scripts": {
		"clean": "tsc -b --clean",
		"compile": "tsc -b",
		"prestart": "npm run compile",
		"start": "node bin/dexnode"
	},
	"dependencies": {
		"semver": "7.3.8",
		"winreg": "1.2.4"
	},
	"devDependencies": {
		"@types/semver": "7.3.13",
		"@types/winreg": "1.2.32",
		"typescript": "4.9.5"
	}
}
